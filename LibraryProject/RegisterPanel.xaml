<Window x:Class="LibraryProject.RegisterPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryProject"
        mc:Ignorable="d"
        Title="Register" Height="300" Width="208.468">
    <Grid>
       
        <Label Content="Username" HorizontalAlignment="Left" Margin="68,36,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="68,107,0,0" VerticalAlignment="Top"/>
        <TextBox Name="PassTxt" HorizontalAlignment="Left" Height="26" Margin="27,133,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="144"/>
        <TextBox Name="UserTxt" HorizontalAlignment="Left" Height="25" Margin="27,62,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="144"/>
        <Button Name="RegisterBtn" Content="Register" HorizontalAlignment="Left" Margin="27,175,0,0" VerticalAlignment="Top" Width="144" Height="34" Click="RegisterBtn_Click"/>
        <Button Name="ExitBtn" Content="Exit" HorizontalAlignment="Left" Margin="27,223,0,0" VerticalAlignment="Top" Width="144" Height="36" Click="ExitBtn_Click"/>

    </Grid>
</Window>
